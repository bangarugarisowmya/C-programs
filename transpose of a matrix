#include <stdio.h>

#define ROW 3
#define COL 3

void transpose(int mat[ROW][COL], int trans[COL][ROW]) {
    for (int i = 0; i < ROW; i++)
        for (int j = 0; j < COL; j++)
            trans[j][i] = mat[i][j];
}

void printMatrix(int mat[ROW][COL]) {
    for (int i = 0; i < ROW; i++) {
        for (int j = 0; j < COL; j++)
            printf("%d ", mat[i][j]);
        printf("\n");
    }
}

int main() {
    int mat[ROW][COL] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };
    int trans[COL][ROW];

    printf("Original:\n");
    printMatrix(mat);

    transpose(mat, trans);
    printf("Transpose:\n");
    printMatrix(trans);

    return 0;
}
OUTPUT :
Original:
1 2 3 
4 5 6 
7 8 9 
Transpose:
1 4 7 
2 5 8 
3 6 9 
