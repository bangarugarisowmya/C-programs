#include <stdio.h>
#include <stdlib.h>
void findRepeating(int arr[], int size) {
    printf("Repeating elements: ");
    for (int i = 0; i < size; i++) {
        if (arr[abs(arr[i])] < 0)
            printf("%d ", abs(arr[i]));
        else
            arr[arr[i]] = -arr[arr[i]];
    }
    printf("\n");
}

int main() {
    int arr[] = {1, 2, 3, 1, 3, 6, 6};
    int size = sizeof(arr) / sizeof(arr[0]);

    findRepeating(arr, size);
    return 0;
}
OUTPUT : 
Repeating elements: 1 3 6 
