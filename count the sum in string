#include <stdio.h>
#include <ctype.h>
int sumNumbers(char* s) {
    int sum = 0, num = 0;
    for(; *s; s++) {
        if(isdigit(*s)) num = num * 10 + (*s - '0');
        else { sum += num; num = 0; }
    }
    return sum + num;
}

int main() {
    char s[] = "a10b20c30";
    printf("Sum: %d", sumNumbers(s)); // Output: 60
    return 0;
}
OUTPUT :
Sum: 60
