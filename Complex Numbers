#include <stdio.h>

typedef struct {
    float r;
    float i;
} Complex;

Complex add(Complex c1, Complex c2) {
    Complex res;
    res.r = c1.r + c2.r;
    res.i = c1.i + c2.i;
    return res;
}

int main() {
    Complex c1 = {2.3, 4.5}, c2 = {1.2, 3.4};
    Complex res = add(c1, c2);
    printf("Sum: %.1f + %.1fi", res.r, res.i);
    return 0;
}

OUTPUT :
Sum: 3.5 + 7.9i
