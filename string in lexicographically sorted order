#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void swap(char* a, char* b) {
    char t = *a;
    *a = *b;
    *b = t;
}

void permute(char* s, int l, int r) {
    if (l == r)
        printf("%s\n", s);
    else {
        for (int i = l; i <= r; i++) {
            swap(s + l, s + i);
            permute(s, l + 1, r);
            swap(s + l, s + i); // backtrack
        }
    }
}

int main() {
    char s[] = "143";
    int n = strlen(s);
    qsort(s, n, 1, (int (*)(const void*, const void*))strcmp);
    permute(s, 0, n - 1);
    return 0;
}

OUTPUT :
134
143
314
341
431
413
